{
	"name": "sunbeam",
	"homepage": "https://tauag.github.io/Sunbeam/",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@babel/polyfill": "^7.2.5",
		"axios": "^0.18.0",
		"normalize.css": "^8.0.1",
		"prop-types": "^15.6.2",
		"react": "^16.8.1",
		"react-dom": "^16.8.1",
		"react-scripts": "^2.1.8",
		"react-transition-group": "^2.5.3",
		"styled-components": "^4.1.3"
	},
	"scripts": {
