		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/preset-env": "^7.3.4",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.8.0",
		"gh-pages": "^2.0.1"
	},
	"jest": {
		"coverageReporters": [
			"json",
			"text"
		]
